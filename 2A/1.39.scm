(define (cont-frac n d k)
  (define (iter a b)
    (if(= a b)
      (/ (n a) (d a))  
      (/ (n a) (+ (d a) (iter (+ a 1) b))) ))
  (iter 1 k))

(define (tan-cf x k) 
  (cont-frac (lambda (i) 
               (if(= i 1)
                 x 
                 (* -1 (square x))))
              (lambda (i) (- (* 2 i) 1) ) 
              k))

